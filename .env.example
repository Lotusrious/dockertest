# Database Configuration
DB_USER=traveluser
DB_PASSWORD=travelpass

# Redis Configuration
REDIS_PASSWORD=redispass

# RabbitMQ Configuration
RABBITMQ_USER=rabbit
RABBITMQ_PASSWORD=rabbitpass

# JWT Configuration
JWT_SECRET=your-secret-key-change-this-in-production
